<Page
    x:Class="AdvComProject.MapOrResource"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:AdvComProject"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <Button x:Name="back_button" Content="Back" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Click="back_button_Click"/>
        <Image HorizontalAlignment="Center" Height="169" Margin="87,110,88,0" VerticalAlignment="Top" Width="225" Source="Assets/map_icon.png" Tapped="map_button"/>
        <Image HorizontalAlignment="Center" Height="170" Margin="110,353,110,0" VerticalAlignment="Top" Width="180" Source="Assets/resource_icon.png" Tapped="resource_button"/>


    </Grid>
</Page>
